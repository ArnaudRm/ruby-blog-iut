<ul>
  <% @users.each do |user| %>
    <li>
      <% if user.avatar? %>
        <%= image_tag user.avatar.thumb.url, alt: "user_avatar" %>
      <% end %>

      <%= link_to "#{user}", user %>
    </li>
  <% end %>
</ul>

<%= link_to "Inscription", new_user_registration_path %>

